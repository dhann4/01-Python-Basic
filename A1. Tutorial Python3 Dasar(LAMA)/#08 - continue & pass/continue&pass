for i in range(1,10):

    if i is 6:
        print("nilai i adalah",6)
        # break : fungsinya untuk mengakhiri for (terminasi)
        # continue : fungsinya untuk melanjutnya ke step berikutnya
        pass
        print("cek bro 1")

    print('nilai saat ini adalah',i)
else:
    print('akhir dari loop')

print('print diluar loop')